    "@tmagic/core": "1.2.13",
    "@tmagic/design": "1.2.13",
    "@tmagic/editor": "1.2.13",
    "@tmagic/element-plus-adapter": "1.2.13",
    "@tmagic/form": "1.2.13",
    "@tmagic/schema": "1.2.13",
    "@tmagic/stage": "1.2.13",
    "@tmagic/utils": "1.2.13",
    "element-plus": "^2.2.32",
    "monaco-editor": "^0.34.0",
    "serialize-javascript": "^6.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.10"
